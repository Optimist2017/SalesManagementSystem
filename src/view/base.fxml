<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="488.0" prefWidth="750.0" style="-fx-background-color: orange;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.baseController">
   <children>
      <HBox alignment="CENTER_RIGHT" spacing="10.0" style="-fx-background-color: gold;">
         <children>
            <Button mnemonicParsing="false" style="-fx-background-color: orange; -fx-background-radius: 10;" text="Sales" onAction="#loadsales" />
            <Button mnemonicParsing="false" style="-fx-background-color: orange; -fx-background-radius: 10;" text="Inventory" onAction="#loadinventory" />
            <Button layoutX="502.0" layoutY="20.0" mnemonicParsing="false" onAction="#loadproduct" style="-fx-background-color: orange; -fx-background-radius: 10;" text="Products" />
            <Button layoutX="707.0" layoutY="20.0" mnemonicParsing="false" onAction="#loadcategories" style="-fx-background-color: orange; -fx-background-radius: 10;" text="Categories" />
            <Button mnemonicParsing="false" onAction="#loadprofile" style="-fx-background-color: orange; -fx-background-radius: 10;" text="Profile" />
            <Button mnemonicParsing="false" style="-fx-background-color: orange; -fx-background-radius: 10;" text="Log Out" />
            <Button layoutX="548.0" layoutY="20.0" mnemonicParsing="false" onAction="#loademployees" style="-fx-background-color: orange; -fx-background-radius: 10;" text="Employees" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <VBox fx:id="container" prefHeight="200.0" prefWidth="100.0" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
